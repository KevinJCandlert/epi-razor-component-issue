<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <Using Include="EPiServer" />
    <Using Include="EPiServer.Core" />
    <Using Include="EPiServer.DataAbstraction" />
    <Using Include="EPiServer.DataAnnotations" />
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="EPiServer.CMS" Version="12.28.0" />
    <PackageReference Include="EPiServer.CMS.AspNetCore.Mvc" Version="12.21.2" />
    <PackageReference Include="EPiServer.CMS.AspNetCore.Routing" Version="12.21.2" />
    <PackageReference Include="EPiServer.CMS.AspNetCore.TagHelpers" Version="12.21.2" />
    <PackageReference Include="EPiServer.Framework" Version="12.21.2" />
  </ItemGroup>
  
  <ItemGroup>
    <EmbeddedResource Include="Resources\Translations\**\*" />
  </ItemGroup>
</Project>
